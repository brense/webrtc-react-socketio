{
  "name": "webrtc-react-socketio",
  "version": "1.0.0",
  "main": "./webrtc/index.js",
  "types": "./webrtc/index.d.ts",
  "author": "Rense Bakker",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brense/webrtc-react-socketio.git"
  },
  "bugs": {
    "url": "https://github.com/brense/webrtc-react-socketio/issues"
  },
  "homepage": "https://github.com/brense/webrtc-react-socketio#readme",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "rxjs": "^7.5.5",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "yargs": "^17.5.0"
  },
  "scripts": {
    "build:npm": "rimraf lib-esm && tsc --p ./tsconfig.npm.json && copyfiles package.json README.md lib-esm && npm publish ./lib-esm",
    "start": "react-scripts start",
    "start:server": "ts-node-dev --project ./tsconfig.server.json ./src/signalingServer.ts",
    "build:app": "react-scripts build",
    "prebuild": "rimraf dist && rimraf build",
    "build": "npm run build:app && tsc --p tsconfig.server.json",
    "postbuild": "copyfiles -u 1 dist/**/* build",
    "deploy": "node ./build/signalingServer.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.7.0",
    "moment": "^2.29.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  }
}
